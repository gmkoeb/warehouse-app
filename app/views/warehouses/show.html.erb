<section>
  <h2>Galpão <%= @warehouse.code %></h2>
  <div>
    <p>Descrição: <%=@warehouse.description%></p>
    <ul>
      <li>Nome: <%= @warehouse.name %></li>
      <li>Cidade: <%= @warehouse.city %></li>
      <li>Área: <%= @warehouse.area %> m²</li>
      <li>Endereço: <%= @warehouse.address %>. CEP: <%= @warehouse.cep %></li>
    </ul>
  </div>
</section>

<div>
  <%= link_to 'Editar', edit_warehouse_path %>
  <%= button_to('Remover', warehouse_path(@warehouse.id), method: :delete) %>
</div>

<div>
  <%= link_to 'Voltar', root_path %>
</div>